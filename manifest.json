{
  "name": "OfficePoltergeist",
  "version": "1.0.0",
  "description": "The Office Poltergeist",
  "permissions": [ "background", "storage", "tts"],

   "background": { "scripts": ["socket.io-1.2.0.js", "underscore-min.js", "client.js"] },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "OfficePoltergeist",
    "default_popup": "popup.html"
  },

   "content_scripts": [ {
      "css": [ "effects.css" ],
      "js": [ "jquery-2.2.3.min.js", "underscore-min.js", "findAndReplaceDOMText.js", "background.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],

   "web_accessible_resources": [
      "media/*.*"
   ],

  "manifest_version": 2
}
